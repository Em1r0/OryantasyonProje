import java.util.Scanner; //Kullanıcıdan sayı alacağımız için Scanner kütüphanesini dahil ediyoruz.

public class ToplamaIslemiDongu{
	public static void main(String[] args){
		
			int sayi1=(int)(Math.random()*100); //Bilgisayarın rastgele bir sayı oluşturması için Math kütüphanesini kullanıyoruz.
			int sayi2=(int)(Math.random()*100); //Bilgisayarın rastgele bir sayı oluşturması için Math kütüphanesini kullanıyoruz.

			Scanner input=new Scanner(System.in);
			
			System.out.print(sayi1 + "+" + sayi2 + "sonucu nedir?"); //Kullanıcıya, bilgisayar tarafından belirlenen sayılarının toplamını girmesini istiyoruz.
			int cevap=input.nextInt(); //Kullanıcıdan aldığımız sonucu bir değişkene atıyoruz.
			
			while(sayi1 + sayi2 != cevap){ //Döngümüz için şartı belirliyoruz. Şart sağlandıkça program döngüye girecektir. 
				System.out.print("Yanlis cevap! Tekrar deneyin.\n" + sayi1 + "+" + sayi2 + "sonucu nedir?"); //Cevap doğru olmadığı için ekrana bir mesaj ve de ardından işlemi yazdırıyoruz.
				cevap=input.nextInt(); //Kullanıcıdan tekrar sonuç için girdi istiyoruz.
			}
			
			System.out.println("Dogru cevap! Tebrikler."); //Döngü şartı sağlanmadığı için program döngüye girmeyip bu kod satırını çalıştırıyor.	
	}
}
