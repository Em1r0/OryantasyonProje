import java.util.Scanner; //Kullanıcıdan sayı alacağımız için Scanner kütüphanesini dahil ediyoruz.

public class GozcuDeger{
	public static void main(String[] args){
		
		Scanner input = new Scanner(System.in);
		
		System.out.print("Bir tam sayi giriniz (0 girilirse giris sonlanir!): "); //Kullanıcıyı yönlendirmek için önce ekrana direktif yazdırıyoruz.
		
		int veri = input.nextInt(); //Kullanıcının girdiği sayıyı tutmak için bir değişken oluşturuyoruz.
		int toplam = 0; //Sayıları toplayacağımız için yeni bir değişken oluşturup "0" değerini atıyoruz. Çünkü toplamada etkisiz eleman sıfırdır.
		
		while(veri != 0){ //Kullanıcıdan alınan sayı "0" olmadıkça döngü devam edecek şekilde şart belirlenir.
			toplam += veri; //Kullanıcıdan alınan sayı toplam değişkenine eklenerek tekrardan toplam değişkenine atanır.
			
			System.out.print("Bir tam sayi giriniz (0 girilirse giris sonlanir!): "); //Kullanıcıdan tekrar sayı almak için ekrana mesaj yazdırıyoruz.
			veri = input.nextInt(); //Kullanıcıdan aldığımız sayıyı veri değişkenimize atıyoruz.
		}
		
		System.out.print("Toplam: " + toplam); /*Kullanıcı sıfır girerek döngü şartının sağlanmamasına sebep olur.
												Program artık döngüye girmeden bu satıra gelerek ekrana toplamı yazdırır ve program sonlanır.*/
		
	}
}
