import java.util.Scanner; //Kullanıcıdan sayı alacağımız için Scanner kütüphanesini dahil ediyoruz.

public class ToplamaIslemiKontrol{
    public static void main(String[] args){
        
        int sayi1=(int) (Math.random()*100); //Bilgisayarın rastgele bir sayı oluşturması için Math kütüphanesini kullanıyoruz.
        int sayi2=(int) (Math.random()*100); //Bilgisayarın rastgele bir sayı oluşturması için Math kütüphanesini kullanıyoruz.
        
        Scanner input=new Scanner(System.in); 
        
        System.out.print(sayi1 + "+" + sayi2 + " sonucu kactir? "); //Kullanıcıya, bilgisayar tarafından belirlenen sayılarının toplamını girmesini istiyoruz.
        
        int cevap=input.nextInt(); //Kullanıcıdan aldığımız sonucu bir değişkene atıyoruz.
        
        System.out.println(sayi1 + "+" + sayi2 + "=" + cevap + " sonucu " + " " + (sayi1+sayi2==cevap)); /*Boolean kullanarak girilen sonuç doğru ise
																										ekrana "true", yanlış ise "false" yazdıran bir mesaj yazdırıyoruz.*/
    }
}
