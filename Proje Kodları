public class DiziTest{
	public static void main(String[] args){
		
		int a[][]={{1,3,5,7},{2,4,6,8}};
		int b[][]={{10,30,50,70},{20,40,60,80}};
		
		int c[][]=new int[2][4];
		
		for(int i=0;i<3;i++){
			for(int j=0;j<4;j++){
				
				c[i][j]=a[i][j]+b[i][j];
				
				System.out.println(c[i][j] + " ");
			}
			
		System.out.println();
		
		}
	}
}
