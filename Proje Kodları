import java.util.Scanner; //Kullanıcıdan sayı alacağımız için Scanner kütüphanesini dahil ediyoruz.

public class TahminOyunu{
	public static void main(String[] args){
		
		int sayi=(int)(Math.random()*100); //Bilgisayarın rastgele bir sayı oluşturması için Math kütüphanesini kullanıyoruz.
		
		Scanner input=new Scanner(System.in);
		
		System.out.println("0 ile 100 arasinda bir sayi tahmin ediniz:"); //Kullanıcıyı yönlendirmek için önce ekrana direktif yazdırıyoruz.
		
		int tahmin; //Kullanıcının yapacağı tahmin için boş bir değişken oluşturuyoruz.
		
		do{
			System.out.println("Tahmininiz: ");
			tahmin=input.nextInt(); //Kullanıcının adlığı sayıyı ekrana yazdırıyoruz.
			
			if(tahmin==sayi) //Kullanıcının tahmini ile bilgisayar tarafından tutulan sayı aynı ise program "if" bloğunu çalıştırıyor.
				System.out.println("Dogru tahmin, sayi:" + sayi); //Tahminin doğru olduğunu ekrana yazdırıyoruz.
			
			else if(tahmin > sayi) //Kullanıcının tahmini, bilgisayar tarafından tutulan sayıdan büyük ise program "else if" bloğunu çalıştırıyor.
				System.out.println("Tahmininiz yuksek"); //Tahminin yüksek olduğunu ekrana yazdırıyoruz.
			
			else //Kullanıcının tahmini, bilgisayar tarafından tutulan sayıdan küçük ise program "else" bloğunu çalıştırıyor.
				System.out.println("Tahmininiz dusuk"); //Tahminin düşük olduğunu ekrana yazdırıyoruz.
			}while(tahmin != sayi); //Kullanıcının tahmini, bilgisayar tarafından tutulan sayıdan farklı ise program "do" bloğuna dönüyor. Döngü şartı yanlış olana kadar döngü devam eder.
	}
}
